{
	"name": "team-manager",
	"version": "1.0.0",
	"description": "Team Manager Backend service",
	"main": "app.ja",
	"keywords": [
		"Advance",
		"api",
		"nodejs",
		"express"
	],
	"author": "Emmanuel Okafor",
	"license": "ISC",
	"engines": {
		"node": "12.20.0",
		"npm": "6.14.4"
	},
	"scripts": {
		"prestart": "npm run -s build",
		"start": "node ./dist/app.js",
		"dev": "nodemon --exec \"npm run lint && babel-node src/app.js\"",
		"prod": "NODE_ENV=production npm-run-all lint build start",
		"build": "npm-run-all clean transpile",
		"transpile": "babel src --out-dir dist",
		"clean": "rimraf dist-server",
		"lint": "eslint -c .eslintrc src",
		"test": "jest --detectOpenHandles"
	},
	"dependencies": {
		"@babel/cli": "^7.13.10",
		"@babel/core": "^7.13.10",
		"@babel/preset-env": "^7.13.10",
		"apollo-errors": "^1.9.0",
		"apollo-resolvers": "^1.4.2",
		"apollo-server-express": "^2.21.1",
		"axios": "^0.21.1",
		"chai": "^4.3.3",
		"config": "^3.3.6",
		"cookie-parser": "^1.4.5",
		"core-js": "^3.9.1",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"faker": "^5.4.0",
		"graphql": "^15.5.0",
		"graphql-log": "^0.1.3",
		"http": "^0.0.1-security",
		"lodash": "^4.17.21",
		"moment": "^2.29.1",
		"moment-timezone": "^0.5.33",
		"mongoose": "^5.11.19",
		"morgan": "^1.10.0",
		"nodemon": "^2.0.7",
		"query-string": "^6.14.1",
		"rimraf": "^3.0.2",
		"url-parse": "^1.5.1",
		"validatorjs": "^3.22.1",
		"winston": "^3.3.3"
	},
	"devDependencies": {
		"@babel/node": "^7.13.10",
		"babel-eslint": "^10.1.0",
		"eslint": "^4.19.1",
		"eslint-config-google": "^0.9.1",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jest": "^24.2.1",
		"eslint-watch": "^3.1.3",
		"jest": "^26.6.3",
		"npm-run-all": "^4.1.2",
		"supertest": "^3.0.0"
	},
	"jest": {
		"testEnvironment": "node",
		"modulePathIgnorePatterns": [
			"/config/"
		],
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		]
	}
}
